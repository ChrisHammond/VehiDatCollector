IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}VehiDataCollector_Vehicles]') AND type in (N'U'))
DROP TABLE {databaseOwner}[{objectQualifier}VehiDataCollector_Vehicles]
GO


IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}VehiDataCollector_Entries]') AND type in (N'U'))
DROP TABLE {databaseOwner}[{objectQualifier}VehiDataCollector_Entries]
GO

